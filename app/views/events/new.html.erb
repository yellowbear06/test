<h1>テスト</h1>

<%= form_with model: @event, local: true do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="name">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div class="place_id">
    <%= f.label :place_id %>
    <%= f.number_field :place_id %>
  </div>
  <div class="time">
    <%= f.label :start_time %>
    <%= f.datetime_select :start_time %>
  </div>
  <div class="time">
    <%= f.label :end_time %>
    <%= f.datetime_select :end_time %>
  </div>
  <div class="content">
    <%= f.label :description %>
    <%= f.text_field :description %>
  </div>
  <div class="url">
    <%= f.label :event_url %>
    <%= f.text_field :event_url %>
  </div>
  <div class="submit">
    <%= f.submit %>
  </div>
<% end %>